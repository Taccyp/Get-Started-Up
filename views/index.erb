	<div id="sidebar">
		<h1>Sign up!</h1>
		<form method="POST" action="/submission" id="signup">
			<label for="first_name">First Name</label>
			<input type="text" name="first_name">
			<label for="last_name">Last Name</label>
			<input type="text" name="last_name">
			<label for="email">Email</label>
			<input type="email" name="email">
			<label for="education">Past Education</label>
			<textarea type="text" name="education" rows="1"></textarea>
			<label for="work_history">Work History</label>
			<textarea type="text" name="work_history" rows="1"></textarea>
			<label for="interests">Other Interests or Hobbies</label>
			<textarea type="text" name="interests" rows="1"></textarea>
			<label for="comment">Anything else you'd like us to know?</label>
			<textarea type="text" name="comment" rows="1"></textarea>
			<input type="submit">
		</form>
	
